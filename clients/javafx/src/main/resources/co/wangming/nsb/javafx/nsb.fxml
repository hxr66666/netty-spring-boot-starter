<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.wangming.nsb.javafx.MainController">
   <children>
      <Pane prefHeight="600.0" prefWidth="165.0">
         <children>
            <Button fx:id="startBtn" layoutY="-1.0" mnemonicParsing="false" onMouseClicked="#onStartButtonClick" prefHeight="43.0" prefWidth="165.0" text="开始" />
            <Button fx:id="endBtn" layoutY="42.0" mnemonicParsing="false" onMouseClicked="#onEndButtonClick" prefHeight="43.0" prefWidth="165.0" text="结束" />
            <Label layoutY="85.0" prefHeight="35.0" prefWidth="43.0" text="线程数" />
            <TextField fx:id="threadNum" layoutY="114.0" prefHeight="26.0" prefWidth="165.0" text="1" />
            <Label layoutY="149.0" prefHeight="35.0" prefWidth="65.0" text="循环次数" />
            <TextField fx:id="cycleNum" layoutY="176.0" prefHeight="26.0" prefWidth="165.0" text="1" />
            <Label layoutY="213.0" prefHeight="35.0" prefWidth="87.0" text="单次沉睡毫秒" />
            <TextField fx:id="sleepNum" layoutX="-1.0" layoutY="241.0" prefHeight="26.0" prefWidth="165.0" text="0" />
            <Label layoutY="274.0" prefHeight="35.0" prefWidth="43.0" text="IP" />
            <TextField fx:id="host" layoutX="-1.0" layoutY="299.0" prefHeight="26.0" prefWidth="165.0" text="127.0.0.1" />
            <Label layoutX="3.0" layoutY="330.0" prefHeight="35.0" prefWidth="43.0" text="端口" />
            <TextField fx:id="port" layoutX="-1.0" layoutY="357.0" prefHeight="26.0" prefWidth="165.0" text="7800" />
            <Label layoutX="3.0" layoutY="389.0" prefHeight="35.0" prefWidth="132.0" text="重建连接间隔次数" />
            <TextField fx:id="rebuildNum" layoutX="-1.0" layoutY="415.0" prefHeight="26.0" prefWidth="165.0" text="0" />
         </children>
      </Pane>
      <ListView fx:id="listView" layoutX="165.0" layoutY="-1.0" prefHeight="600.0" prefWidth="635.0" />
   </children>
</AnchorPane>
